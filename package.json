{
  "name": "my-notes",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prettier": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "pinia": "^2.0.9",
    "three": "^0.139.0",
    "vue": "^3.2.25",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/dat.gui": "^0.7.7",
    "@types/node": "^17.0.10",
    "@types/three": "^0.138.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "dat.gui": "^0.7.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "sass": "^1.49.0",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**": [
      "prettier --config .prettierrc --write",
      "eslint --fix",
      "git add"
    ]
  }

}
